int dSeek(int x,int y, int d, int mD, int pX, int pY)
{
  int tempx,tempy,res;
  if(x > pX || y > pY){return 0 ;}
  if(d >= mD){return 1;}
  tempx = x + d;
  tempy = y + d;
  d = d << 1;
  return (dSeek(tempx,y,d,mD,pX,pY) + dSeek(x, tempy, d,mD, pX, pY))+1;
}
